{
  "name": "in2",
  "version": "0.1.0",
  "description": "Scenario editor for the 'in2' project.",
  "main": "src/index.js",
  "readme": "readme",
  "private": true,
  "repository": {
    "type": "https",
    "url": "https://github.com/benjamin-t-brown/in2/tree-builder"
  },
  "dependencies": {
    "jsplumb": "^2.4.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "uglify-js": "^2.6.1",
    "uglifycss": "0.0.26",
    "watchify": "^3.9.0",
    "minimist": "^1.2.0"
  },
  "config": {
    "vendor_modules": [
      "react",
      "react-dom"
    ],
    "bundles": {
      "vendor": "build/vendor.js",
      "main": {
        "out": "build/main.js",
        "entry": "src/index.js"
      }
    }
  },
  "scripts": {
    "build-vendor": "node build.js build-vendor",
    "build-onboard": "node build.js build-onboard",
    "build": "node build.js build",
    "build-maps": "node build.js build-maps",
    "build-css": "node build.js build-css",
    "watch": "node build.js watch",
    "run": "node src-srv",
    "clean": "node build.js clean",
    "start": "node server"
  },
  "author": "Benjamin Brown",
  "license": "UNLICENSED"
}
